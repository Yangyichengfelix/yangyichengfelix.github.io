@if (StringInit.Any())
{   
    @foreach (char letter in StringInit)
    {
        if (char.IsWhiteSpace(letter))
        {
            <MudInputString Disabled></MudInputString>
        }
        else
        {
            <MudInputString ></MudInputString>
        }
    }
    @for (int i = 0; i < StringInit.Length; i++)
    {
        if (char.IsWhiteSpace(StringInit[i]))
        {
            <MudInputString Disabled></MudInputString>
        }
        else
        {
            <MudInputString ></MudInputString>
        }
    }
}

